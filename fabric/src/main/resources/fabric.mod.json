{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "sources": "https://github.com/Raik176/CyberRewared",
    "issues": "https://github.com/Raik176/CyberRewared/issues"
  },
  "license": "${mod_license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.rhm.CyberRewaredFabric"
    ],
    "client": [
      "org.rhm.CyberRewaredFabric"
    ],
    "fabric-datagen": [
      "org.rhm.util.CyberRewaredDataGenerator"
    ],
    "jei_mod_plugin": [
      "org.rhm.integration.jei.CWJEIPlugin"
    ],
    "modmenu": [
      "org.rhm.integration.CWModMenuPlugin"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=21",
    "fabric-api": "*",
    "jei": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/sstFzru8Se"
      }
    }
  }
}
